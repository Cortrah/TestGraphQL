<p id="notice"><%= notice %></p>

<h1>Listing Unit Action Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Description template</th>
      <th>Results template</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unit_action_types.each do |unit_action_type| %>
      <tr>
        <td><%= unit_action_type.name %></td>
        <td><%= unit_action_type.code %></td>
        <td><%= unit_action_type.description_template %></td>
        <td><%= unit_action_type.results_template %></td>
        <td><%= link_to 'Show', unit_action_type %></td>
        <td><%= link_to 'Edit', edit_unit_action_type_path(unit_action_type) %></td>
        <td><%= link_to 'Destroy', unit_action_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

